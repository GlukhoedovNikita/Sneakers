{
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"webpack --watch\" \"nodemon dist/main.js\"",
    "build": "webpack",
    "lint": "eslint src --ext .ts --ext .js",
    "lint:fix": "eslint src --ext .ts --ext .js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".(ts|js)": [
      "npm run lint:fix",
      "git add ."
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "node": "^18.5.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack-cli": "^4.10.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "wepback": "^1.0.0"
  }
}
